<html>
<head><style>
table, th, td {
  border: 1px solid #111;
  padding: 5px;
  vertical-align:text-top;
  font-size:12px;
  letter-spacing:0px;
  font-family:sans-serif;
}
table {
  border-spacing: 15px;
}
img {
  border: 3px double orange;
}
body {
  color: white;
  background-color: black;
}
</style></head>
<body>

<table style="width:100%">
  <div id="reports">
  <tr>
    <td>
      <h1><%= opts[:name] %></h1>
      <b>Processed</b>: <%= nice_bytes(ent.bytes) %><br>
      <b>Most Freq Byte</b>: <%= '%2.2f%%' % (ent.byte_count.max * 25600.0 / ent.bytes) %><br>
      <b>Least Freq Byte</b>: <%= '%2.2f%%' % (ent.byte_count.min * 25600.0 / ent.bytes) %><br>
    </td>
    <td><b>Covariance</b><br><img src="covariance.png"></td>
    <td><b>Byte Frequency</b><br><img src="byte.png"></td>
    <td><b>Bit Frequency</b><br><img src="bit.png"></td>
    <td><b>P-Values</b><br><img src="pvalues.png"></td>
  </tr>
  </div>
</table>

</body></html>
